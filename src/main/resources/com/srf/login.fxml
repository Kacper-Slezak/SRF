<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.srf.controllers.LoginController">
   <VBox layoutX="223.0" layoutY="66.0" prefWidth="155.0" spacing="10.0">
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
      <Label alignment="CENTER" contentDisplay="CENTER" prefWidth="150.0" text="Log in" textAlignment="CENTER">
         <font>
            <Font size="27.0" />
         </font>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets bottom="20.0" />
         </padding></Label>
      <Label text="Username" />
      <TextField fx:id="usernameTextField" />
      <Label text="Password" />
      <PasswordField fx:id="passwordField" />
      <Button fx:id="logInButton" mnemonicParsing="false" onAction="#onLogInButton" prefWidth="150.0" text="Log in">
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin></Button>
      <Label text="Don't have an account?" />
      <Button fx:id="registerButton" mnemonicParsing="false" onAction="#onRegisterButton" text="Register" />
   </VBox>
</AnchorPane>
